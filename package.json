    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "emoji-js": "^3.4.0",
    "emoji-picker-react": "^2.0.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^4.0.1",
