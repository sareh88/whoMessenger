    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jsonwebtoken": "^8.1.1",
    "jwt-decode": "^2.2.0",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "less-vars-to-js": "1.2.1",
