{
  "name": "whoMessenger",
  "version": "1.0.0",
  "main": "app.js",
  "engines": {
    "node": "v8.11.1"
  },
  "scripts": {
    "start": "concurrently --kill-others  \"yarn run server\"  \"yarn run socket\" \"yarn run react\"",
    "heroku-postbuild": "npm run build",
    "server": "nodemon app.js",
    "socket": "nodemon index.js",
    "react": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint src/components/*"
  },
  "repository": "git@github.com:sareh88/whoMessenger.git",
  "author": "Sareh Almaaz <sareh.almaaz@comjoo.com>",
  "license": "MIT",
  "private": true,
